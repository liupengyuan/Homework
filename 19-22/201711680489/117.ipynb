   "source": [
    "import random,math\n",
    "\n",
    "m=int(input('请随意输入一个整数，回车结束'))\n",
    "n=int(input('请随意输入不同于m的整数，回车结束'))\n",
    "k=int(input('请随意输入一个不同于m，n的整数，回车结束'))\n",
    "\n",
    "i=0\n",
    "total=0\n",
    "while i<m:\n",
    "    number=random.randint(n,k)\n",
    "    i=i+1\n",
    "    total=total+number\n",
    "    break\n",
    "total=math.sqrt(total)\n",
    "print(total)"
   ]
