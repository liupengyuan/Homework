    "import random\n",
    "def sum():\n",
    "    m=int(input('请输入个数m'))\n",
    "    n=int(input('请输入一个整数n'))\n",
    "    k=int(input('请输入一个大于n的整数k'))\n",
    "    \n",
    "    \n",
    "    i=0\n",
    "    x=0\n",
    "    total=0\n",
    "    while i<m:\n",
    "        i+=1\n",
    "        x=random.randint(n,k)\n",
    "        total+=x\n",
    "        answer=total**(1/2)\n",
    "    return answer\n",
    "print(sum())"
