   "source": [
    "#输出【1,1024】之间所有能被7整除的偶数的和。\n",
    "i = 1\n",
    "total=0\n",
    "n=0\n",
    "while i <= 1024:\n",
    "    if i % 7 > 0 or i % 2 > 0:\n",
    "        i = i + 1\n",
    "    else:\n",
    "        n = i\n",
    "        total = total + n\n",
    "        i = i + 1\n",
    "print('【1,1024】之间所有能被7整除的偶数的和是：',total)"
