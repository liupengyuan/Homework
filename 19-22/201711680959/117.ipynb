    "i=0\n",
    "s=0\n",
    "m=int(input('请输入您需要的整数个数：'))\n",
    "n=int(input('请输入整数下限：'))\n",
    "k=int(input('请输入整数上限：'))\n",
    "while i<=m:\n",
    "    i=i+1\n",
    "    a=random.randint(n,k)\n",
    "    s=s+a\n",
    "result=math.sqrt(a)\n",
    "print('随机数和的平方根是：',result)\n"
