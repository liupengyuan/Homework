    "#输出1到1024之间所有能被7整除的偶数的和\n",
    "n=1024\n",
    "i=1\n",
    "s=0\n",
    "while i<=n: \n",
    "    i=i+1\n",
    "    if i%7==0 and i%2==0:\n",
    "        s=s+i\n",
    "print(s)\n",
    "    \n",
    "    "
   ]
