    "import random\n",
    "def total(m,n,k):\n",
    "    total=0\n",
    "    i=1\n",
    "    while i<=m:    \n",
    "        i=i+1\n",
    "        number = random.randint(n,k)\n",
    "        total=total+number\n",
    "    return total\n",
    "\n",
    "n=int(input('输入一个较小的数'))\n",
    "k=int(input('输入一个较大的数'))\n",
    "m=int(input('输入相加随机数的个数'))\n",
    "\n",
    "print(total(m,n,k)**(1/2))\n",
    " "
