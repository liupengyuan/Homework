   "source": [
    "#输出【1,1024】之间所有能被7整除的偶数的和\n",
    "\n",
    "i = 1\n",
    "total = 0\n",
    "\n",
    "while i <= 1024:\n",
    "    if i % 7 == 0 and i % 2 == 0:\n",
    "        total += i\n",
    "    i += 1\n",
    "    \n",
    "print (total)"
   ]
  }
