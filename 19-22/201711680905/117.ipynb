    "import random\n",
    "def num():\n",
    "    m=int(input())\n",
    "    n=int(input())\n",
    "    k=int(input())\n",
    "    i=1\n",
    "    total=0\n",
    "    if n>k:\n",
    "        a=n\n",
    "        n=k\n",
    "        k=a\n",
    "    while i<=m:\n",
    "        s=random.randint(n,k)\n",
    "        total+=s\n",
    "        i+=1\n",
    "        return total\n",
    "    print(total**(1/2))\n",
    "# 主程序\n",
    "num()"
   ]
