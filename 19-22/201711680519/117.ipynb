    "import random\n",
    "def main(m,n,k):\n",
    "    i=0\n",
    "    total=0\n",
    "    while i<m:\n",
    "        number=random.randint(n,k)\n",
    "        i+=1\n",
    "        total+=number\n",
    "        result=total**(1/2)\n",
    "        print(number)\n",
    "    print('这m个数和的平方根是',result)\n",
    "    \n",
    "   \n",
    "main(5,10,500)"
   ]
