    "import random,math\n",
    "m=int(input('请输入一个整数,表示你要取m个整数'))\n",
    "n=int(input('请输入一个正数'))\n",
    "k=int(input('请输入一个大于n的正数'))\n",
    "def a(m,n,k):\n",
    "    i=0\n",
    "    total=0\n",
    "    \n",
    "    while i<m:\n",
    "        number=random.randint(n,k)\n",
    "        total+=number\n",
    "        x=math.sqrt(total)\n",
    "    print(x)\n",
    "    \n",
    "\n",
    "a(5,10,500)"
   ]
