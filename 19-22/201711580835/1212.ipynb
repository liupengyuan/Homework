    "import random\n",
    "fh = open(r\"C:\\temp\\idioms (1).txt\")\n",
    "n=1\n",
    "text = fh.read()\n",
    "text1= text.replace('\\n', '')\n",
    "idioms=text1.replace('、','')\n",
    "while n<10:\n",
    "    for i in range(10*n):\n",
    "        idiom=random.choice(idioms)\n",
    "        print(idiom,end='')\n",
    "    n=n+1"
