    "import random,math\n",
    "def all(m,n,k):\n",
    "    i=1\n",
    "    total=0\n",
    "    while i<m:\n",
    "        i=i+1\n",
    "        total=random.randint(n,k)+total\n",
    "    return total\n",
    "    \n",
    "m=int(input('请输入你所需整数的个数'))\n",
    "n=int(input('请输入整数最小值，并以回车结束'))\n",
    "k=int(input('请输入整数最大值，并以回车结束'))\n",
    "print(math.sqrt(all(m,n,k)))"
   ]
