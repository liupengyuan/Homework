    "def my_sum(m,n,k):\n",
    "    import random\n",
    "    i = 1\n",
    "    total = 0\n",
    "    while i <= m :\n",
    "        i += 1\n",
    "        number = random.randint(n,k)\n",
    "        total += number\n",
    "    return total\n",
    "\n",
    "m = int(input())\n",
    "n = int(input())\n",
    "k = int(input())\n",
    "a = 1/2\n",
    "print(my_sum(m,n,k)**a)"
   ]
