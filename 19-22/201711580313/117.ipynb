    "m = int(input(\"请输入你想输入的整数个数，回车结束。\"))\n",
    "print(\"请输入一个数字区间(n,k)\")\n",
    "n = int(input(\"请输入区间起点，回车结束。\"))\n",
    "k = int(input('请输入区间终点，回车结束。'))\n",
    "i = 1\n",
    "total = 0\n",
    "import random\n",
    "\n",
    "while i < m:\n",
    "    \n",
    "    j = random.randint(n,k)\n",
    "    total += j\n",
    "    i += 1\n",
    "s = (total)**(1/2)\n",
    "print(s)"
