    "\n",
    "def mul(m,n,k):\n",
    "    i=0\n",
    "    total=0\n",
    "    while i<m:\n",
    "        i+=1\n",
    "        x=random.randint(n,k)\n",
    "        total+=x\n",
    "        \n",
    "    print(total**(1/2))\n",
    "mul(5,10,500)"
