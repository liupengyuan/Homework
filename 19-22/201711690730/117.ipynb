   "source": [
    "#利用计算机得到m个从n到k的随机整数,求这些数的和的平方根\n",
    "import math,random\n",
    "def game():\n",
    "    m=int(input())\n",
    "    n=int(input())\n",
    "    k=int(input())\n",
    "\n",
    "    i=0\n",
    "    total=0\n",
    "    while i<m:\n",
    "        a=random.randint(n,k)\n",
    "        total=total+a\n",
    "        i=i+1\n",
    "    return math.sqrt(total)\n",
    "    \n",
    "game()"
   ]
  }
