   "source": [
    "输出1-1024之间所有能整除7的数之和"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "12\n"
     ]
    }
   ],
   "source": [
    "n=int(input(1))\n",
    "total=0\n",
    "while n<=1024:\n",
    "    m=n%7\n",
    "    if m!=0:\n",
    "        n=n+1\n",
    "    elif m==0:\n",
    "        total=total+n\n",
    "print(total)"
   ]
